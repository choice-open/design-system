    {
      "id": "collections-contextmenu--context-menu-nested-dropdown",
      "name": "Context Menu Nested Dropdown",
      "exportName": "ContextMenuNestedDropdown",
      "description": "",
      "source": "import { ContextMenu } from \"@choice-ui/context-menu\"\n\nexport default function App() {\n  return (\n    <div className=\"flex h-64 items-center justify-center\">\n      <div\n        ref={triggerRef}\n        className=\"bg-secondary-background rounded-xl border border-dashed p-8\"\n      >\n        <Dropdown>\n          <Dropdown.Trigger>\n            <Dropdown.Value>Left/Right click for different menus</Dropdown.Value>\n          </Dropdown.Trigger>\n          <Dropdown.Content>\n            <ContextMenuContent />\n          </Dropdown.Content>\n        </Dropdown>\n      </div>\n\n      <ContextMenu triggerRef={triggerRef}>\n        <ContextMenu.Content>\n          <ContextMenuContent />\n        </ContextMenu.Content>\n      </ContextMenu>\n    </div>\n  );\n}"
