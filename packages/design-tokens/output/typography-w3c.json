{
  "font": {
    "families": {
      "default": {
        "$type": "fontFamily",
        "$value": [
          "Inter",
          "ui-sans-serif",
          "system-ui",
          "-apple-system",
          "BlinkMacSystemFont",
          "Segoe UI",
          "Roboto",
          "Helvetica Neue",
          "Arial",
          "Noto Sans",
          "sans-serif",
          "SFProLocalRange"
        ]
      },
      "display": {
        "$type": "fontFamily",
        "$value": [
          "Whyte",
          "ui-sans-serif",
          "system-ui",
          "-apple-system",
          "BlinkMacSystemFont",
          "Segoe UI",
          "Roboto",
          "Helvetica Neue",
          "Arial",
          "Noto Sans",
          "sans-serif"
        ]
      },
      "mono": {
        "$type": "fontFamily",
        "$value": [
          "Roboto Mono",
          "Monaco",
          "Courier New",
          "SFMono-Regular",
          "Menlo",
          "Consolas",
          "monospace"
        ]
      }
    },
    "weights": {
      "light": {
        "$type": "fontWeight",
        "$value": 400
      },
      "default": {
        "$type": "fontWeight",
        "$value": 450
      },
      "medium": {
        "$type": "fontWeight",
        "$value": 450
      },
      "semibold": {
        "$type": "fontWeight",
        "$value": 500
      },
      "strong": {
        "$type": "fontWeight",
        "$value": 550
      },
      "heavy": {
        "$type": "fontWeight",
        "$value": 550
      }
    },
    "sizes": {
      "xs": {
        "$type": "dimension",
        "$value": {
          "value": 0.5625,
          "unit": "rem"
        }
      },
      "sm": {
        "$type": "dimension",
        "$value": {
          "value": 0.6875,
          "unit": "rem"
        }
      },
      "md": {
        "$type": "dimension",
        "$value": {
          "value": 0.8125,
          "unit": "rem"
        }
      },
      "lg": {
        "$type": "dimension",
        "$value": {
          "value": 0.9375,
          "unit": "rem"
        }
      },
      "xl": {
        "$type": "dimension",
        "$value": {
          "value": 1.5,
          "unit": "rem"
        }
      },
      "2xl": {
        "$type": "dimension",
        "$value": {
          "value": 3,
          "unit": "rem"
        }
      }
    },
    "line-heights": {
      "tight": {
        "$type": "dimension",
        "$value": {
          "value": 0.875,
          "unit": "rem"
        }
      },
      "snug": {
        "$type": "dimension",
        "$value": {
          "value": 1,
          "unit": "rem"
        }
      },
      "normal": {
        "$type": "dimension",
        "$value": {
          "value": 1.375,
          "unit": "rem"
        }
      },
      "relaxed": {
        "$type": "dimension",
        "$value": {
          "value": 1.5625,
          "unit": "rem"
        }
      },
      "loose": {
        "$type": "dimension",
        "$value": {
          "value": 2,
          "unit": "rem"
        }
      },
      "extra-loose": {
        "$type": "dimension",
        "$value": {
          "value": 3.5,
          "unit": "rem"
        }
      }
    },
    "letter-spacings": {
      "body-large": {
        "$type": "dimension",
        "$value": {
          "value": -0.002,
          "unit": "rem"
        }
      },
      "body-medium": {
        "$type": "dimension",
        "$value": {
          "value": 0.00344,
          "unit": "rem"
        }
      },
      "body-small": {
        "$type": "dimension",
        "$value": {
          "value": 0.00281,
          "unit": "rem"
        }
      },
      "body-small-strong": {
        "$type": "dimension",
        "$value": {
          "value": 0.016875,
          "unit": "rem"
        }
      },
      "heading-large": {
        "$type": "dimension",
        "$value": {
          "value": -0.0255,
          "unit": "rem"
        }
      },
      "heading-medium": {
        "$type": "dimension",
        "$value": {
          "value": -0.00469,
          "unit": "rem"
        }
      },
      "heading-small": {
        "$type": "dimension",
        "$value": {
          "value": -0.002,
          "unit": "rem"
        }
      },
      "display": {
        "$type": "dimension",
        "$value": {
          "value": -0.09,
          "unit": "rem"
        }
      }
    }
  },
  "typography": {
    "body": {
      "large": {
        "$type": "typography",
        "$description": "body typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.md}",
          "fontWeight": "{font.weights.default}",
          "lineHeight": "{font.line-heights.normal}",
          "letterSpacing": "{font.letter-spacings.body-large}"
        }
      },
      "large-strong": {
        "$type": "typography",
        "$description": "body typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.md}",
          "fontWeight": "{font.weights.strong}",
          "lineHeight": "{font.line-heights.normal}",
          "letterSpacing": "{font.letter-spacings.body-large}"
        }
      },
      "medium": {
        "$type": "typography",
        "$description": "body typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.sm}",
          "fontWeight": "{font.weights.medium}",
          "lineHeight": "{font.line-heights.snug}",
          "letterSpacing": "{font.letter-spacings.body-medium}"
        }
      },
      "medium-strong": {
        "$type": "typography",
        "$description": "body typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.sm}",
          "fontWeight": "{font.weights.heavy}",
          "lineHeight": "{font.line-heights.snug}",
          "letterSpacing": "{font.letter-spacings.body-medium}"
        }
      },
      "small": {
        "$type": "typography",
        "$description": "body typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.xs}",
          "fontWeight": "{font.weights.default}",
          "lineHeight": "{font.line-heights.tight}",
          "letterSpacing": "{font.letter-spacings.body-small}"
        }
      },
      "small-strong": {
        "$type": "typography",
        "$description": "body typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.xs}",
          "fontWeight": "{font.weights.strong}",
          "lineHeight": "{font.line-heights.tight}",
          "letterSpacing": "{font.letter-spacings.body-small-strong}"
        }
      }
    },
    "heading": {
      "display": {
        "$type": "typography",
        "$description": "heading typography preset",
        "$value": {
          "fontFamily": "{font.families.display}",
          "fontSize": "{font.sizes.2xl}",
          "fontWeight": "{font.weights.light}",
          "lineHeight": "{font.line-heights.extra-loose}",
          "letterSpacing": "{font.letter-spacings.display}"
        }
      },
      "large": {
        "$type": "typography",
        "$description": "heading typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.xl}",
          "fontWeight": "{font.weights.heavy}",
          "lineHeight": "{font.line-heights.loose}",
          "letterSpacing": "{font.letter-spacings.heading-large}"
        }
      },
      "medium": {
        "$type": "typography",
        "$description": "heading typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.lg}",
          "fontWeight": "{font.weights.heavy}",
          "lineHeight": "{font.line-heights.relaxed}",
          "letterSpacing": "{font.letter-spacings.heading-medium}"
        }
      },
      "small": {
        "$type": "typography",
        "$description": "heading typography preset",
        "$value": {
          "fontFamily": "{font.families.default}",
          "fontSize": "{font.sizes.md}",
          "fontWeight": "{font.weights.heavy}",
          "lineHeight": "{font.line-heights.normal}",
          "letterSpacing": "{font.letter-spacings.heading-small}"
        }
      }
    }
  }
}